<!DOCTYPE html>
<html lang="en">

<head>
	<link href="https://fonts.googleapis.com/css2?family=Ubuntu:wght@300&display=swap" rel="stylesheet">
	<link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css"
		integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p" crossorigin="anonymous" />
	<style>
		@page {
			size: A4 portrait;
			margin: 0;
		}

		/*!
        * HTML-Sheets-of-Paper (https://github.com/delight-im/HTML-Sheets-of-Paper)
        * Copyright (c) delight.im (https://www.delight.im/)
        * Licensed under the MIT License (https://opensource.org/licenses/MIT)
        */

		@import "sheets-of-paper.css";

		.page {
			/* Styles for better appearance on screens only -- are reset to defaults in print styles later */

			/* Reflect the paper width in the screen rendering (must match size from @page rule) */
			width: 21cm;
			/* Reflect the paper height in the screen rendering (must match size from @page rule) */
			min-height: 29.7cm;

			/* Reflect the actual page margin/padding on paper in the screen rendering (must match margin from @page rule) */
			padding-left: 2cm;
			padding-top: 2cm;
			padding-right: 2cm;
			padding-bottom: 2cm;
		}
	</style>

	<style>
		:root {
			--p-font-length: 11px;
			--h-font-length: 11px;
			--title-font-length: 14px;
			--subtitle-font-length: 12px;
		}

		.medium {
			--p-font-length: 13px;
			--h-font-length: 13px;
			--title-font-length: 16px;
			--subtitle-font-length: 14px;

		}

		.small {
			--p-font-length: 11px;
			--h-font-length: 11px;
			--title-font-length: 14px;
			--subtitle-font-length: 12px;
		}

        body,html {
            font-family: 'Ubuntu', 'Courier New', Courier, monospace;
            padding: 0;
            margin: 0;
        }


        .wrapper-padding-top {
            padding-top: 20px;
            padding-bottom: 20px;
        }

        .title {
            font-size: var(--title-font-length);
        }

        .sub-title {
            font-size: var(--subtitle-font-length);
        }


        .text {
            font-size: var(--p-font-length);
        }

        header {
            background-color: #313c4e;
            padding: 30px 30px 10px 30px;
            color: white;
        }

        header .subtitle {
            position: relative;
            display: inline-block;
        }

        header .subtitle::after {
            content: '';
            position: absolute;
            bottom: 0;
            left: 0;
            height: 3px;
            background-color: #fff;
            width: calc(100% + 80px);
            border-radius: 5px; 
        }

        section{
            display: flex;
            padding: 10px;
            padding-top: 20px;
        }

        .sub-heading {
            position: relative;
            display: inline-block;
        }

        .sub-heading::after {
            content: '';
            background-color: #313c4e;
            position: absolute;
            width: 100%;
            height: 2px;
            bottom: 0;
            left: 0;
            border-radius: 5px;
        }

        .icon {
            margin-right: 10px;
            display: inline-block;
        }

        .list-position {
			position: relative;
			top: -1px;
		}

        .column {
            margin-right: 15px;
        }

        .column .section h2{
            position:relative;
        }

        .column .section h2::before{
            content: '';
            width: 100px;
            height: 1px;
            position: absolute;
            background-color:  #313c4e;
            bottom: 0;
        }
	</style>
</head>

<body>
	<div class="medium">
        <header>
            <div class="title">
                <% if (typeof titleName != "undefined") { %>
                    <h1 style="margin: 0; margin-bottom: 5px;"><%= titleName %></h1>
                <% } %>
            </div>
            <div class="subtitle">
                <% if (typeof titleProfession != "undefined") { %>
                    <h3 style="margin: 5px 0;"><%= titleProfession %></h3>
                <% } %>
            </div>

            <div class="description">
                <% if (typeof titleDescription != "undefined") { %>
                    <p stlye="font-weight: bold;"><%= titleDescription %></p>
                <% } %>
            </div>
        </header>

        <section>
            <div style="flex-basis: 280px; padding: 0 10px;">
                <div>
                    <div style="display: flex; align-items: center;">
                        <span class="fas fa-envelope icon text"></span>
                        <% if (typeof email != "undefined") { %>
                            <p class="text"><%= email %></p>
                        <% } %>
                    </div>
    
                    <div style="display: flex; align-items: center;">
                        <span class="fas fa-mobile icon text"></span>
                        <% if (typeof number != "undefined") { %>
                            <p class="text"><%= number %></p>
                        <% } %>
                    </div>
    
                    <div style="display: flex; align-items: center;">
                        <span class="fas fa-map-pin icon"></span>
                        <% if (typeof location != "undefined") { %>
                            <p class="text"><%= location %></p>
                        <% } %>
                    </div>
                </div>

                <% if (typeof skills != "undefined") { %>
                    <div style="margin-top: 20px;" class="skills-section">
                        <h2 class="sub-heading title">SKILLS</h2>
                        <div class="skills">
                            <% for(let skill of skills) { %>
                                <span style="display: inline-block; background-color: rgb(143, 138, 138); color: #fff; padding: 5px 10px; border-radius: 5px; margin: 3px; font-size: var(--p-font-length);"><%= skill.name %></span>
                            <% } %>
                        </div>
                    </div>
                <% } %>

                <% if (typeof languages != "undefined") { %>
                    <div style="margin-top: 20px;" class="skills-section">
                        <h2 class="sub-heading title">LANGUGES</h2>
                        <div class="skills" style="margin-top: 5px;">
                            <ul style="padding: 0 0 0 5px;margin: 0; list-style-type: none;">
                                <% for(let language of languages) { %>
                                    <li style="margin-bottom: 10px">
                                        <div>
                                            <p style="font-size: var(--p-font-length); margin: 0; font-weight: bold;">
                                                <%= language.name %>
                                            </p>
                                        </div>
                                    </li>
                                <% } %>
                            </ul>
                        </div>
                    </div>
                <% } %>
            </div>

            <% if (typeof columns != "undefined" && columns.length === 1) { %>

                <% for(let column of columns) { %>

                    <div style="flex: 1" class="column">
                        <% for(let item of column) { %>
                            <% if (item.type === "CustomSection") { %>
                                <div class="section">
                                    <h2 style="margin: 0; padding-bottom: 1px; "><%= item.title %></h2>

                                    <% for(let section of item.section) { %>
                                        <div style="margin-bottom: 5px;">
                                            <p style="font-weight:800; font-size: var(--subtitle-font-length); margin: 3 0;"><%= section.title %></p>
                                            <p style="margin-bottom: 5px; font-size: var(--subtitle-font-length)"><%= section.subtitle %></p>
                                            <div style="font-size: 10px;display: flex; justify-content: space-between; margin-bottom: 3px;">
                                                <!-- <p style="margin: 0">12/2019 - 3/2021</p> -->
                                                <p style="margin: 0"><%= section.location %></p>
                                            </div>
                                            <div style="font-size: 10px;display: flex; justify-content: space-between; margin-bottom: 3px;">
                                                <!-- <p style="margin: 0">Project and Task</p> -->
                                            </div>

                                            <ul style="margin-left: 18px; padding: 0;">
                                                <% for(let list of section.list) { %>
                                                    <li class="mb-4px">
                                                        <div style="margin-left: -6px;" class="set-list-position">
                                                            <p>
                                                                <% if(list.heading){ %>
                                                                    <span style="font-weight: bold; font-size: var(--h-font-length);">
                                                                        <%= list.heading %>
                                                                    </span>
                                                                    <span>-</span>
                                                                <% } %>
                                                                <span style="font-size: var(--p-font-length);">
                                                                    <%= list.paragraph %>
                                                                </span>
                                                            </p>
                                                        </div>
                                                    </li>
                                                <% } %>
                                            </ul>

                                        </div>
                                    <% } %>

                                </div>
                            <% } %>
                            <% if(item.type === "IntrestSection") { %>
                                <div style="margin-bottom: 35px;" class="section">
                                    <h2 style="margin: 0; padding-bottom: 1px; "><%= item.title %></h2>
            
                                    <div class="toast" style="margin-top: 15px;">
                                        <% for(let list of item.list) { %>
                                            <span style="display: inline-block; border: 2px solid #ccc; color: #000; padding: 5px 10px; border-radius: 5px; margin: 3px; font-size: var(--p-font-length);"><%= list.name %></span>
                                        <% } %>
                                    </div>
                                </div>
                            <% } %> 
